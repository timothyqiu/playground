; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

[application]

config/name="Turn Based Strategy Demo"
run/main_scene="res://World.tscn"
config/icon="res://assets/icon.png"

[audio]

default_bus_layout="res://resources/default_bus_layout.tres"

[debug]

gdscript/warnings/unused_argument=false

[display]

window/size/width=320
window/size/height=240
window/size/test_width=640
window/size/test_height=480
window/stretch/mode="viewport"
window/stretch/aspect="keep"

[locale]

translations=PoolStringArray(  )

[physics]

common/enable_pause_aware_picking=true

[rendering]

quality/intended_usage/framebuffer_allocation=0
quality/intended_usage/framebuffer_allocation.mobile=1
2d/snapping/use_gpu_pixel_snap=true
2d/options/ninepatch_mode=0
environment/default_environment="res://resources/default_env.tres"
quality/dynamic_fonts/use_oversampling=false
