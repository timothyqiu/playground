-- ui

function addfloat(t,x,y,c)
	local f={
		txt=t,
		x=x,
		y=y,
		ty=y-8,
		c=c,
		dur=26,
	}
	add(floats,f)
end

function dofloats()
	for f in all(floats) do
		oprint8(f.txt,f.x,f.y,f.c,0)
		f.y+=(f.ty-f.y)/10
		f.dur-=1
		if f.dur<=0 then
			del(floats,f)
		end
	end
end
