function _init()
	t=0
	p_t=0
	dirx={-1,1,0,0,1,1,-1,-1}
	diry={0,0,-1,1,-1,1,1,-1}
	
	debugfirst=true
	debug={}
	
	floats={}
	
	flyseeds={}
	
	start_game()
end

function _update()
	t+=1
	_upd()
end

function _draw()
	_drw()
	dofloats()
	
--	color(8)
--	cursor()
--	for d in all(debug) do
--		print(d)
--	end
end

function start_game()
	holes=make_holes()
	p_cur=1
	ai_cur=8
	
	_drw=draw_game
	
	if rnd()<0.5 then
		player_choose()
	else
		ai_choose()
	end
	
	sfx(6)
end


