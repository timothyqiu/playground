-- draw

function draw_game()
	cls()
	
	print("cpu",2,43,6)
	print("you",114,80,6)
	
	for i=1,#holes do
		local h=holes[i]
		local hx,hy=h.x,h.y
		rect(hx,hy,hx+h.w-1,hy+h.h-1,6)
		local hs=h.seeds
		if hs>0 then
			print(hs,
									hx+(hs>9 and 2 or 4),
									hy+(i%7==0 and 10 or 4))
		end
	end
	
	for n in all(flyseeds) do
		oprint8(n.t,n.x,n.y,6,0)
	end
	
	if hand then
		oprint8(hand.seeds,hand.x,hand.y,9,0)
	end
	
	if cur then
		local pos=gethandpos(cur)
		spr(cur>7 and 1 or 2,
						pos.x-1,
						pos.y+sin(time()))
	end
	
end

function draw_end()

	draw_game()

	print("press ❎ to restart",
						26,100,1)

end
