-- tools

function oprint8(t,x,y,ct,co)
	for i=1,8 do
		print(t,x+dirx[i],y+diry[i],co)
	end
	print(t,x,y,ct)
end

function getrnd(items)
	return items[1+flr(rnd(#items))]
end

function smooth(x)
	return x*x*x*(x*(x*6-15)+10)
end
